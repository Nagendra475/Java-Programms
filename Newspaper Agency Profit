import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt(); 
        int b = scanner.nextInt(); 
        int c = scanner.nextInt(); 
        int rev = a * b; 
        int cost = a * c + 100; 
        int profit = rev - cost; 
        System.out.println(profit);
    }
}
